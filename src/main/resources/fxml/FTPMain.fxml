<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="526.0" prefWidth="697.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlControllers.FTPMain">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints valignment="CENTER" vgrow="NEVER" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <MenuBar fx:id="mainMenuBar">
                    <menus>
                        <Menu fx:id="menuFile" mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="fileAdmin" mnemonicParsing="false" text="Admin" />
                                <MenuItem fx:id="fileLogout" mnemonicParsing="false" text="Logout" />
                                <MenuItem fx:id="fileClose" mnemonicParsing="false" text="Close" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <GridPane GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Slider fx:id="scroller" focusTraversable="false" orientation="VERTICAL" prefHeight="322.0" prefWidth="22.0" GridPane.columnIndex="1">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                        </Slider>
                        <AnchorPane prefHeight="322.0" prefWidth="985.0">
                            <children>
                                <TreeTableView fx:id="ftable" focusTraversable="false" prefHeight="322.0" prefWidth="578.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TreeTableColumn fx:id="fname" prefWidth="178.0" text="Filename" />
                                        <TreeTableColumn fx:id="fsize" prefWidth="203.0" text="Filesize" />
                                        <TreeTableColumn fx:id="ftype" prefWidth="104.0" text="Filetype" />
                                        <TreeTableColumn fx:id="dadded" prefWidth="121.0" text="Date Added" />
                                    </columns>
                                    <columnResizePolicy>
                                        <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                    </columnResizePolicy>
                                </TreeTableView>
                            </children>
                        </AnchorPane>
                    </children>
                </GridPane>
                <AnchorPane prefHeight="200.0" prefWidth="697.0" GridPane.rowIndex="2">
                    <children>
                        <GridPane layoutX="14.0" layoutY="13.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0" />
                                <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="10.0" />
                                <ColumnConstraints halignment="LEFT" hgrow="NEVER" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Button fx:id="uploadButton" mnemonicParsing="false" onAction="#handleUpload" text="Upload">
                                    <GridPane.margin>
                                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                    </GridPane.margin>
                                </Button>
                                <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" GridPane.columnIndex="2">
                                    <GridPane.margin>
                                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                    </GridPane.margin>
                                </Button>
                                <Button fx:id="shareButton" mnemonicParsing="false" text="Share" GridPane.columnIndex="1">
                                    <GridPane.margin>
                                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                    </GridPane.margin>
                                </Button>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
